## Send an error

<form>
  <input type="text" bind:value={msg1} placeholder="error message here.." />
  <button disabled={!msg1} on:click={ async (ev) => {
    console.log("Processing...", ev);
      //
    const st = await dispatchError_INTERNAL( msg1 );
    console.log(`Error was ${ st ? 'delivered':'queued' }`);
    msg1 = '';
  } }>Send!!</button>
</form>

<script>
import { dispatchError_INTERNAL } from '@local/rg4esm';

import { name, version } from "@local/rg4esm/package.json";
  //
  const [PACKAGE_NAME, PACKAGE_VERSION] = [name, version];

const RAYGUN_API_KEY = import.meta.env.VITE_RAYGUN_API_KEY;

let msg1 = "abc";
</script>

